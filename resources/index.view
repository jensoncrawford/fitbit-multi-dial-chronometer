          </g>
          <g id="hrHand" transform="translate(50%,50%)">
            <line x1="0" y1="0" x2="0" y2="-30%" stroke-width="2" class="minuteColor"/>
          </g>
      </use>
    </section>

    <!-- Clock Face -->
    <g transform="translate(50%,50%)">
      <use href="#maindial"/>
      <g id="hourhand"><line x1="0" y1="4%" x2="0" y2="-30%" stroke-width="5" class="minuteColor"/></g>
      <g id="minutehand"><line x1="0" y1="4%" x2="0" y2="-39%" stroke-width="5" class="minuteHandColor"/></g>
      <g id="secondhand"><line x1="0" y1="4%" x2="0" y2="-39%" stroke-width="3" class="secondHandColor"/></g>
      <circle id="outercenterdot" cx="0" cy="0" r="6" class="secondHandColor"/>
      <circle id="innercenterdot" cx="0" cy="0" r="3" class="minuteHandColor"/>
    </g>
